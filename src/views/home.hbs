<img src="/img/image.webp" alt="person looking out through window" class="hero-image" width="981" height="528">

<section class="articles">

    <h2 class="articles__heading">Latest Posts</h2>
    <ul class="article-ul">
        {{#each blogdata}}
        <li>
            <a href="/post/{{this.id}}">
                <span>{{this.title}}</span>
                <span class="article-list__date">{{this.createAt}}</span>
            </a>
        </li>
        {{/each}}


    </ul>


    <a href="#" class="pagination">&lt; View Older Posts</a>
</section>